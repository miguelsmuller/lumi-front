# Editor configuration, see https://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,jsx,ts,tsx}]
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false
